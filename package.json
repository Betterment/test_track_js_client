{
  "name": "@betterment/test-track",
  "type": "module",
  "version": "2.1.3",
  "description": "Javascript Client for Test Track",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "pnpm run /^dev:.*/",
    "dev:build": "tsdown --watch",
    "dev:server": "python -m http.server 8000",
    "build": "tsdown",
    "typecheck": "tsc",
    "lint": "eslint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "release": "release-it",
    "test": "vitest run"
  },
  "contributors": [
    "Ryan O'Neill <ryan.oneill@betterment.com>",
    "Alex Burgel <aburgel@betterment.com>",
    "Adam Langsner <adam.langsner@gmail.com>",
    "Joe Jansen <jansen@betterment.com>"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/js-cookie": "^3.0.3",
    "eslint": "^9.39.2",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "prettier": "3.7.4",
    "release-it": "^16.1.2",
    "tsdown": "^0.18.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "js-cookie": "^2.2.1",
    "js-md5": "^0.8.3",
    "uuid": "^13.0.0"
  },
  "files": [
    "dist/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Betterment/test_track_js_client.git"
  },
  "keywords": [
    "split testing",
    "feature flagging",
    "test track"
  ],
  "release-it": {
    "hooks": {
      "before:init": "pnpm build"
    }
  },
  "packageManager": "pnpm@10.26.2+sha512.0e308ff2005fc7410366f154f625f6631ab2b16b1d2e70238444dd6ae9d630a8482d92a451144debc492416896ed16f7b114a86ec68b8404b2443869e68ffda6"
}
