{
  "name": "test_track_js_client",
  "type": "module",
  "version": "2.1.3",
  "description": "Javascript Client for Test Track",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "pnpm run /^dev:.*/",
    "dev:build": "tsdown --watch",
    "dev:server": "python -m http.server 8000",
    "build": "tsdown",
    "typecheck": "tsc",
    "lint": "eslint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "release": "release-it",
    "test": "vitest run"
  },
  "contributors": [
    "Ryan O'Neill <ryan.oneill@betterment.com>",
    "Alex Burgel <aburgel@betterment.com>",
    "Adam Langsner <adam.langsner@gmail.com>",
    "Joe Jansen <jansen@betterment.com>"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/blueimp-md5": "^2.18.0",
    "@types/js-cookie": "^3.0.3",
    "@types/qs": "^6.9.7",
    "@types/uuid": "^9.0.2",
    "eslint": "^9.39.2",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "prettier": "3.7.4",
    "release-it": "^16.1.2",
    "tsdown": "^0.18.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "abab": "^2.0.3",
    "axios": "^1.5.0",
    "blueimp-md5": "2.10.0",
    "js-cookie": "^2.2.0",
    "qs": "^6.9.1",
    "uuid": "^9.0.0"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Betterment/test_track_js_client.git"
  },
  "keywords": [
    "split testing",
    "feature flagging",
    "test track"
  ],
  "release-it": {
    "hooks": {
      "before:init": "pnpm build"
    }
  }
}
